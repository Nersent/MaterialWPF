<Window x:Class="MaterialWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaterialWPF"
        xmlns:materialWpf="clr-namespace:MaterialWPF;assembly=MaterialWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1024"
        Background="#F5F5F5"
        WindowStyle="None"
        >
    <Window.Resources>
        <Style TargetType="ContextMenu">
            
        </Style>
    </Window.Resources>
    <Grid x:Name="Main">
        <materialWpf:TabLayout TabSize="300" TabDrag="Enabled" x:Name="tabLayout" Margin="0,48,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        <materialWpf:ToolBar x:Name="ToolBarx" Width="Auto" Height="48" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="0,0,0,0"></materialWpf:ToolBar>
    </Grid>

</Window>