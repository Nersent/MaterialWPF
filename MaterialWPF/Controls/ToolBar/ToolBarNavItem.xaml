<UserControl x:Class="MaterialWPF.ToolBarNavItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MaterialWPF"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="32">
    <Grid x:Name="MainGrid" ClipToBounds="True">
        <Grid x:Name="line1" Background="Black" Width="32" Height="2.5" Margin="0,8,0,0" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Grid x:Name="line2" Background="Black" Width="32" Height="2.5" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Grid x:Name="line3" Background="Black" Width="32" Height="2.5" Margin="0,0,0,8" HorizontalAlignment="Center" VerticalAlignment="Bottom" RenderTransformOrigin="0.5,0.5">

        </Grid>
    </Grid>
</UserControl>
